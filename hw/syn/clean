#!/bin/tcsh

# Loop through all files in the current directory
foreach file (*)
    # Check if the file is not 'clean' or 'run_vcs'
    if ("$file" != "clean" && "$file" != "run" && "$file" != "run.tcl" && "$file" != "dut.constraints.tcl") then
        # Remove the file
        rm -rf "$file"
    endif
end
